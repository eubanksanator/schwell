<p id="notice"><%= notice %></p>

<div class="col-md-12">
  <div class="row">
  <h3 class="col-md-6">Name</h3>
    <h3 class="col-md-6">Number</h3>
  </div>
  <div class="row">
    <% @contacts.each do |contact| %>
    <p class="col-md-6"><%= contact.name %></p>
    <p class="col-md-6"><%= contact.phone_num %></p>
    <% end %>
  </div>
</div>

<div class="row">
  <%= form_tag import_contacts_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import" %>
  <% end %>
</div>

<%= link_to 'New Contact', new_contact_path %>
